  "enableAutoPublish" : false,
  "type" : "synonymDetection",
  "outputCollection" : "rc2_query_rewrite_staging",
  "overlapThreshold" : 0.5,
  "fieldToVectorize" : "query_s",
  "randomSeed" : 1234,
  "minQueryCount" : 5,
  "trainingCollection" : "rc2_signals_aggr",
  "keyPhraseCollection" : "rc2_query_rewrite_staging",
  "misspellingsFilterQuery" : "type:spell",
  "misspellingCollection" : "rc2_query_rewrite_staging",
  "overwriteOutput" : true
